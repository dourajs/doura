{
  "name": "doura",
  "version": "0.0.0-rc.0",
  "description": "A Redux Framework",
  "main": "lib/index.js",
  "module": "esm/index.js",
  "browser": "esm/index.js",
  "umd:main": "dist/doura.umd.production.min.js",
  "unpkg": "dist/doura.umd.production.min.js",
  "types": "esm/index.d.ts",
  "sideEffects": false,
  "scripts": {
    "dev": "run-p watch:*",
    "watch:esm": "tsc -p tsconfig.build.json -m esnext --outDir esm -w",
    "watch:cjs": "tsc -p tsconfig.build.json -m commonjs --outDir lib -w",
    "build": "run-p clean && run-p build:*",
    "build:esm": "tsc -p tsconfig.build.json -m esnext --outDir esm",
    "build:cjs": "tsc -p tsconfig.build.json -m commonjs --outDir lib",
    "build:umd": "dts build --format umd --tsconfig ./tsconfig.build.json --name doura",
    "clean": "rimraf lib esm dist"
  },
  "authors": [
    "liximomo"
  ],
  "keywords": [
    "doura",
    "state management",
    "javascript",
    "typescript"
  ],
  "homepage": "https://dourajs.github.io/doura",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dourajs/doura.git"
  },
  "bugs": {
    "url": "https://github.com/dourajs/doura/issues"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  },
  "files": [
    "esm",
    "lib",
    "dist"
  ],
  "devDependencies": {
    "lodash": "^4.17.4"
  }
}
