{
  "name": "doura-plugin-persist",
  "version": "0.0.0-rc.1",
  "main": "lib/index.js",
  "module": "esm/index.js",
  "browser": "esm/index.js",
  "types": "esm/index.d.ts",
  "sideEffects": false,
  "scripts": {
    "dev": "run-p watch:*",
    "watch:esm": "tsc -p tsconfig.build.json -m esnext --outDir esm -w",
    "watch:cjs": "tsc -p tsconfig.build.json -m commonjs --outDir lib -w",
    "build": "run-p clean && run-p build:*",
    "build:esm": "tsc -p tsconfig.build.json -m esnext --outDir esm",
    "build:cjs": "tsc -p tsconfig.build.json -m commonjs --outDir lib",
    "clean": "rimraf lib esm"
  },
  "authors": [
    "liximomo"
  ],
  "keywords": [
    "doura",
    "state management",
    "javascript",
    "typescript"
  ],
  "homepage": "https://dourajs.github.io/doura.org",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dourajs/doura.git"
  },
  "bugs": {
    "url": "https://github.com/dourajs/doura/issues"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  },
  "files": [
    "esm",
    "lib"
  ],
  "engines": {
    "node": ">=10"
  },
  "dependencies": {},
  "devDependencies": {
    "doura": "workspace:*"
  },
  "peerDependencies": {
    "doura": "0.0.0-rc.1"
  }
}
